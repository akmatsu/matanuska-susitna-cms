    "express-session": "^1.18.1",
    "install": "^0.13.0",
    "ioredis": "^5.6.1",
    "next": "^14.2.24",
    "next-auth": "^4.24.11",
    "openid-client": "^5.7.1",
    "passport": "^0.7.0",
    "pluralize": "^8.0.0",
    "react": "^18.3.1",
